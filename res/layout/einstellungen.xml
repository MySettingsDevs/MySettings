<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:weightSum="1">

	<RelativeLayout android:layout_width="fill_parent"
		android:orientation="horizontal" android:layout_height="wrap_content"
		android:paddingTop="10dp" style="@android:style/ButtonBar">
		
		<TextView android:textAppearance="?android:attr/textAppearanceLarge" 
		android:id="@+id/einstellungen" 
		android:layout_width="wrap_content" 
		android:text="@string/Einstellungen" 
		android:layout_height="wrap_content" 
		android:layout_centerVertical="true" 
		android:layout_alignParentLeft="true" 
		android:layout_marginLeft="92dp">
		</TextView>

	</RelativeLayout>
	<LinearLayout android:layout_width="match_parent" android:id="@+id/linearLayout1" android:layout_height="wrap_content" android:weightSum="1">
		<TextView android:id="@+id/textView1" android:textAppearance="?android:attr/textAppearanceMedium" android:layout_height="wrap_content" android:text="Ordner SD-Karte" android:layout_width="140dp" ></TextView>
		<EditText android:layout_weight="1" android:layout_width="wrap_content" android:id="@+id/ordner_sdkarte" android:layout_height="wrap_content" android:text="@string/MySettings"></EditText>
		<Button android:text="OK" android:layout_width="wrap_content" android:id="@+id/ordner_sd_karte_save_button" android:layout_height="wrap_content"></Button>
	</LinearLayout>
	
	<LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/linearLayout2">
		<TextView android:id="@+id/textView2" android:textAppearance="?android:attr/textAppearanceMedium" android:layout_height="wrap_content" android:text="CM7Icons" android:layout_width="140dp"></TextView>
		<ToggleButton android:id="@+id/CM7Icons" android:layout_height="wrap_content" android:text="ToggleButton" android:layout_width="match_parent"></ToggleButton>
	</LinearLayout>
	
	<LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/linearLayout2">
		<TextView android:id="@+id/textView3" android:textAppearance="?android:attr/textAppearanceMedium" android:layout_height="wrap_content" android:text="Circle" android:layout_width="140dp"></TextView>
		<ToggleButton android:id="@+id/Circle" android:layout_height="wrap_content" android:text="ToggleButton" android:layout_width="match_parent"></ToggleButton>
	</LinearLayout>
	
	<LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/linearLayout2">
		<TextView android:id="@+id/textView4" android:textAppearance="?android:attr/textAppearanceMedium" android:layout_height="wrap_content" android:text="Quad" android:layout_width="140dp"></TextView>
		<ToggleButton android:id="@+id/Quad" android:layout_height="wrap_content" android:text="ToggleButton" android:layout_width="match_parent"></ToggleButton>
	</LinearLayout>
	
	<LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/linearLayout2">
		<TextView android:id="@+id/textView5" android:textAppearance="?android:attr/textAppearanceMedium" android:layout_height="wrap_content" android:text="CM7taskswitcher" android:layout_width="140dp"></TextView>
		<ToggleButton android:id="@+id/CM7taskswitcher" android:layout_height="wrap_content" android:text="ToggleButton" android:layout_width="match_parent"></ToggleButton>
	</LinearLayout>

	<LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/linearLayout2">
		<TextView android:id="@+id/textView6" android:textAppearance="?android:attr/textAppearanceMedium" android:layout_height="wrap_content" android:text="CM9taskswitcher" android:layout_width="140dp"></TextView>
		<ToggleButton android:id="@+id/CM9taskswitcher" android:layout_height="wrap_content" android:text="ToggleButton" android:layout_width="match_parent"></ToggleButton>
	</LinearLayout>

	<LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/linearLayout2">
		<TextView android:id="@+id/textView7" android:textAppearance="?android:attr/textAppearanceMedium" android:layout_height="wrap_content" android:text="SamsungApps" android:layout_width="140dp"></TextView>
		<ToggleButton android:id="@+id/SamsungApps" android:layout_height="wrap_content" android:text="ToggleButton" android:layout_width="match_parent" ></ToggleButton>
	</LinearLayout>
</LinearLayout>